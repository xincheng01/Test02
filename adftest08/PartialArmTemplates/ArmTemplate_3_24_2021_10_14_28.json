							"retry": 0,
							"retryIntervalInSeconds": 30,
							"secureOutput": false,
							"secureInput": false
						},
						"userProperties": [],
						"typeProperties": {
							"notebookPath": "/Users/zhang_xincheng8@hotmail.com/synapse/1.Azure-Synapse-Analytics"
